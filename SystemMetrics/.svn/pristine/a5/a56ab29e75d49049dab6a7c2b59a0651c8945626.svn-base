package com.gcaa.metrics.domain.model;

public class HardDisk extends Resource {

	private String name;
	
	public HardDisk(double used, double total, String name) {
		super(used, total);
		this.name = name;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	@Override
	public String toString() {
		return "HardDisk [name=" + name + ", toString() of =" + super.toString() + "]";
	}
	
	
	
	
}

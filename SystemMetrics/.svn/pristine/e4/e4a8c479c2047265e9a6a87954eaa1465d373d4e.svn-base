package com.gcaa.metrics.domain.model;


public enum DiskType {
	
	LOCAL_DISK("Local Disk"),
	CD_DRIVE("CD Drive"),
	NETWORK_DRIVE("Network Drive")
	;
	
	private DiskType(String code) {
		this.code=code;
	}
	
	private String code;
	
	public String getCode() {
		return code;
	}
	public void setCode(String code) {
		this.code = code;
	}
	
}
